n = 5
for i in range (1,n+1):
    if(i == 1):
        temp = 1
    elif(i == 2):  
        temp = 3
    elif(i == 3): 
        temp = 4
    elif(i == 4):
        temp = 10
    else:
        temp = 11
    for j in range(1,i+1):
        print(temp,end=" ")
        if(i % 2 == 0):
          temp -= 1
        else:
          temp += 1
    print()    
    
