n = 5;t = 1;h = 1;c = 1
for i in range (0,n):
    l = t
    for j in range (n,i,-1):
        print(t,end=" ")
        t+=2
    t = l + 2
    if( i > 0):
        for j in range (0,i):
            print(h,end=" ")
            h -= 2
        c += 2    
        h = c 
    print()


